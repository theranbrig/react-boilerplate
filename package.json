{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "Theran Brigowatz React Boilerplate",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build-babel": "npm run build-babel-server && npm run build-babel-client",
    "build-babel-server": "babel src/server --out-dir ./dist",
    "build-babel-client": "babel src/client --copy-files --out-dir ./dist/public",
    "clean": "rimraf dist node_modules",
    "start": "node ./dist/server.js",
    "test": "jest ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theranbrig/react-boilerplate.git"
  },
  "keywords": [
    "node",
    "react"
  ],
  "author": "Theran Brigowatz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/theranbrig/react-boilerplate/issues"
  },
  "homepage": "https://github.com/theranbrig/react-boilerplate#readme",
  "dependencies": {
    "express": "^4.16.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "rimraf": "^2.6.2",
    "webpack": "^3.12.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.4.0",
    "webpack-node-externals": "^1.7.2"
  }
}
